{
  "expo": {
    "name": "Social Networking",
    "slug": "rn-social-networking",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/instaIcon.png",
    "splash": {
      "image": "./assets/ionicons.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "notification": {
      "icon": "./assets/instaIcon.png",
      "color": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "useNextNotificationsApi": true,
      "package": "com.networking.social",
      "versionCode": 3,
      "googleServicesFile": "./google-services.json"
    }
  }
}
